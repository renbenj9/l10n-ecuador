<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="l10n_ec_electronic_document_imported_comp_rule" model="ir.rule">
        <field name="name">Ecuadorian Electronic Document Imported Multi Company</field>
        <field name="model_id" ref="model_l10n_ec_electronic_document_imported" />
        <field
            name="domain_force"
        >['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
    </record>
    <record id="l10n_ec_electronic_document_imported_line_comp_rule" model="ir.rule">
        <field
            name="name"
        >Ecuadorian Electronic Document Imported line Multi Company</field>
        <field name="model_id" ref="model_l10n_ec_electronic_document_imported_line" />
        <field name="domain_force">
            ['|',('document_id.company_id','=',False),('document_id.company_id', 'in', company_ids)]
        </field>
    </record>
</odoo>
